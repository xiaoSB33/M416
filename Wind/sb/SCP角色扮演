    local httpService = game:GetService("HttpService")
    local avatarJson = httpService:JSONDecode(avatarData)
    if avatarJson and avatarJson.data and avatarJson.data[1] then
        avatarUrl = avatarJson.data[1].imageUrl
    end
end

