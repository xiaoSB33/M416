LOL:Toggle(
    "ÊòæÁ§∫Á©∫‰∫ëËÑöÊú¨‰ΩúËÄÖÔºàÊ£ÄÊµãÂú®‰∏çÂú®ÊúçÂä°Âô®‰∏≠Ôºâ",
    "",
    true,
    function(Value)
        if Value then
            local creatorName = "CN_917686666"
            local Players = game:GetService("Players")
            local LocalPlayer = Players.LocalPlayer
            
            local function createBillboard(player)
                if player.Name == creatorName and player ~= LocalPlayer then
                    local character = player.Character or player.CharacterAdded:Wait()
                    local head = character:WaitForChild("Head")
                    
                    local billboard = Instance.new("BillboardGui")
                    billboard.Name = "CreatorTag"
                    billboard.Adornee = head
                    billboard.Size = UDim2.new(0, 100, 0, 40)
                    billboard.StudsOffset = Vector3.new(0, 3, 0)
                    billboard.AlwaysOnTop = true
                    billboard.Parent = head
                    
                    local label = Instance.new("TextLabel")
                    label.Size = UDim2.new(1, 0, 1, 0)
                    label.BackgroundTransparency = 1
                    label.Text = "Á©∫‰∫ëËÑöÊú¨ÂºÄÂèëËÄÖ"
                    label.TextColor3 = Color3.fromRGB(0, 255, 0)
                    label.TextSize = 14
                    label.Font = Enum.Font.GothamBold
                    label.Parent = billboard
                    
                    player.CharacterAdded:Connect(function(newChar)
                        billboard.Adornee = newChar:WaitForChild("Head")
                    end)
                    
                    return billboard
                end
            end
            
            local function checkAndCreate()
                for _, player in pairs(Players:GetPlayers()) do
                    if player.Name == creatorName and player ~= LocalPlayer then
                        createBillboard(player)
                    end
                end
            end
            
            checkAndCreate()
            
            local playerAddedConn = Players.PlayerAdded:Connect(function(player)
                if player.Name == creatorName and player ~= LocalPlayer then
                    createBillboard(player)
                end
            end)
            
            local playerRemovingConn = Players.PlayerRemoving:Connect(function(player)
                if player.Name == creatorName and player.Character then
                    local head = player.Character:FindFirstChild("Head")
                    if head then
                        local billboard = head:FindFirstChild("CreatorTag")
                        if billboard then
                            billboard:Destroy()
                        end
                    end
                end
            end)
            
            _G.CreatorDisplay = {
                PlayerAdded = playerAddedConn,
                PlayerRemoving = playerRemovingConn
            }
        else
            if _G.CreatorDisplay then
                _G.CreatorDisplay.PlayerAdded:Disconnect()
                _G.CreatorDisplay.PlayerRemoving:Disconnect()
                _G.CreatorDisplay = nil
            end
            
            local Players = game:GetService("Players")
            for _, player in pairs(Players:GetPlayers()) do
                if player.Character then
                    local head = player.Character:FindFirstChild("Head")
                    if head then
                        local billboard = head:FindFirstChild("CreatorTag")
                        if billboard then
                            billboard:Destroy()
                        end
                    end
                end
            end
        end
    end
)
