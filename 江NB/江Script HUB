    s.Door.ClientOpen:FireServer()
    s.Door.ClientOpen:FireServer()
          task.wait(0.4)
          tp(s.RoomEnd.CFrame)
             s.Door.ClientOpen:FireServer()
             s.Door.ClientOpen:FireServer()
          elseif s.Name=="100" then
            s.ChildAdded:Connect(function(d)
                
        task.wait(0.6)
          tp(d.Door.Hidden.CFrame)
      end)
             else
             task.wait(0.3)
          tp(s.RoomStart.CFrame)
                s.Door.ClientOpen:FireServer()
                s.Door.ClientOpen:FireServer()
                    s.Door.ClientOpen:FireServer()
                s.Door.ClientOpen:FireServer()
          task.wait(0.3)
          tp(s.RoomEnd.CFrame)
              s.Door.ClientOpen:FireServer()
                s.Door.ClientOpen:FireServer()
             s.Door.ClientOpen:FireServer() 
             s.Door.ClientOpen:FireServer()
      end
              end)
           end)
